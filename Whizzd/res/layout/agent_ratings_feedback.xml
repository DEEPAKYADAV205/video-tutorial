<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="20">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:weightSum="5"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/agent_rating_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFF00" >

            <TextView
                android:id="@+id/agent_rating_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="My Ratings"
                android:textAppearance="?android:attr/textAppearanceLarge" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/agent_delivery_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4" >

        <RelativeLayout
            android:id="@+id/order_delivery_layout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:background="#FFFFFF"
            android:padding="10dp">

            <LinearLayout
                android:id="@+id/order_delivery_linear1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="#9ACD32"
                android:paddingTop="3dp"
                android:paddingRight="3dp"
                android:paddingLeft="3dp"
                android:paddingBottom="1dp"
                android:weightSum="3" >

                <EditText
                    android:id="@+id/name_edittextlabel"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_gravity="fill"
                    android:layout_weight="1"
                    android:background="#D3D3D3"
                    android:clickable="false"
                    android:gravity="center"
                    android:inputType="none"
                    android:text="Total Orders"
                    android:textStyle="bold" >
                </EditText>

                <EditText
                    android:id="@+id/name_edittext"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="1dp"
                    android:layout_weight="1"
                    android:background="#D3D3D3"
                    android:gravity="center"
                    android:inputType="none"
                    android:paddingLeft="3dp"
                    android:text="Total Ratings"
                    android:textStyle="bold" >
                </EditText>

                <EditText
                    android:id="@+id/eta_edittext"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="1dp"
                    android:layout_weight="1"
                    android:background="#D3D3D3"
                    android:layout_gravity="fill"
                    android:gravity="center"
                    android:inputType="none"
                    android:text="Rating"
                    android:textStyle="bold" >
                </EditText>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/order_delivery_linear2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/order_delivery_linear1"
                android:orientation="horizontal"
                android:background="#9ACD32"
                android:paddingBottom="3dp"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:weightSum="3" >

            <EditText
                android:id="@+id/name1_edittextlabel"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_gravity="fill"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:clickable="false"
                android:gravity="center"
                android:inputType="none"
                android:paddingLeft="3dp"
                android:text="475" >
            </EditText>

            <EditText
                android:id="@+id/name1_edittext"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginLeft="1dp"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:layout_gravity="center"
                android:gravity="center"
                android:inputType="none"
                android:text="43" >
            </EditText>

            <EditText
                android:id="@+id/eta1_edittext"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginLeft="1dp"
                android:layout_weight="1"
                android:background="#FFFFFF"
                android:gravity="center"
                android:inputType="none"
                android:text="4.2/5" >
            </EditText>
            </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>

        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:weightSum="5"
        android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/agent_feedback_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#FFFF00" >

            <TextView
                android:id="@+id/agent_feedback_label"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="Orders Delivered"
                android:textAppearance="?android:attr/textAppearanceLarge" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/agent_delivery1_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4" >

            <RelativeLayout
                android:id="@+id/order_delivery1_layout3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="#FFFFFF"
                android:padding="10dp">

                <LinearLayout
                    android:id="@+id/order_delivery1_linear1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:background="#9ACD32"
                    android:paddingTop="3dp"
                    android:paddingRight="3dp"
                    android:paddingLeft="3dp"
                    android:paddingBottom="1dp"
                    android:weightSum="4" >

                    <EditText
                        android:id="@+id/name_edittextlabel1"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_gravity="fill"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:clickable="false"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="1"
                        android:textStyle="bold">
                    </EditText>

                    <EditText
                        android:id="@+id/name_edittext1"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="3"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:inputType="none"
                        android:paddingLeft="3dp"
                        android:text="Late, but properly dressed" >
                    </EditText>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/order_delivery2_linear2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/order_delivery1_linear1"
                    android:orientation="horizontal"
                    android:background="#9ACD32"
                    android:paddingBottom="3dp"
                    android:paddingLeft="3dp"
                    android:paddingRight="3dp"
                    android:weightSum="4" >

                    <EditText
                        android:id="@+id/name1_edittextlabel1"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_gravity="fill"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:clickable="false"
                        android:gravity="center"
                        android:inputType="none"
                        android:paddingLeft="3dp"
                        android:text="2"
                        android:textStyle="bold">
                    </EditText>

                    <EditText
                        android:id="@+id/name1_edittext1"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="3"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="Good Behaviour" >
                    </EditText>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/order_delivery_button_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:padding="5dp"
        android:layout_weight="2"
        android:gravity="bottom"
        android:background="#55000000"
        android:layout_alignParentBottom="true" >

        <Button
            android:id="@+id/agent_feedback_return_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:background="@drawable/round3"
            android:layout_marginRight="2dp"
            android:layout_toLeftOf="@+id/agent_feedback_button"
            android:text="Return" />

        <Button
            android:id="@+id/agent_feedback_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerInParent="true"
            android:text="" />

        <Button
            android:id="@+id/agent_feedback_see_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@drawable/round3"
            android:layout_toRightOf="@+id/agent_feedback_button"
            android:text="See More" />
    </RelativeLayout>

</LinearLayout>
                    </EditText>

                    <EditText
                        android:id="@+id/eta3_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="75 mins" >
                    </EditText>

                    <EditText
                        android:id="@+id/pending3_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="60 mins" >
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/order_delivery_linear5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/order_delivery_linear4"
                    android:orientation="horizontal"
                    android:background="#9ACD32"
                    android:paddingBottom="1dp"
                    android:weightSum="5" >

                    <EditText
                        android:id="@+id/name4_edittextlabel"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_gravity="fill"
                        android:layout_weight="1.5"
                        android:background="#FFFFFF"
                        android:clickable="false"
                        android:gravity="center"

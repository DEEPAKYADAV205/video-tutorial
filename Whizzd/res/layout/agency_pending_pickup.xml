                    <EditText
                        android:id="@+id/name2_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1.5"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:inputType="none"
                        android:paddingLeft="2dp"
                        android:text="New Ashok Nagar" >
                    </EditText>

                    <EditText
                        android:id="@+id/eta2_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="45 mins" >
                    </EditText>

                    <EditText
                        android:id="@+id/pending2_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:gravity="center"
                        android:inputType="none"
                        android:text="15 mins" >
                    </EditText>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/order_delivery_linear4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/order_delivery_linear3"
                    android:orientation="horizontal"
                    android:background="#9ACD32"
                    android:paddingBottom="1dp"
                    android:weightSum="5" >

                    <EditText
                        android:id="@+id/name3_edittextlabel"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_gravity="fill"
                        android:layout_weight="1.5"
                        android:background="#FFFFFF"
                        android:clickable="false"
                        android:gravity="center"
                        android:text="Deepak Yadav"
                        android:inputType="none"
                        android:paddingLeft="2dp" >
                    </EditText>

                    <EditText
                        android:id="@+id/name3_edittext"
                        android:layout_width="0dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="1dp"
                        android:layout_weight="1.5"
                        android:background="#FFFFFF"
                        android:layout_gravity="center"

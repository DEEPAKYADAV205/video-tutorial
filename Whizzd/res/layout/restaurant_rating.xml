                android:singleLine="true"
                android:gravity="center"
                android:text="Rating" />

            <EditText
                android:id="@+id/tde_edittext022"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginBottom="1dp"

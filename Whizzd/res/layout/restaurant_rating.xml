                android:gravity="center"
                android:text="3.5" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/sublinearpost33"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="#9ACD32"
            android:orientation="vertical"
            android:paddingBottom="3dp"
            android:paddingRight="1dp"
            android:paddingTop="3dp" >

            <EditText
                android:id="@+id/tde_edittext0111"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                android:background="#D3D3D3"
                android:singleLine="true"
                android:gravity="center"
                android:text="Comment" />

            <EditText
                android:id="@+id/tde_edittext0222"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginBottom="1dp"
                android:background="#ffffff"
                android:inputType="textMultiLine"                
                android:gravity="center"
                android:singleLine="true"
                android:text="Delicious" />

            <EditText
                android:id="@+id/tde_edittext0333"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#ffffff"
                android:inputType="textMultiLine"
                android:gravity="center"
                android:singleLine="true"
                android:text="Spicy" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/sublinearpost44"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#9ACD32"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingBottom="3dp"
            android:paddingRight="3dp"
            android:paddingTop="3dp" >

            <LinearLayout
                android:id="@+id/sublayoutpost2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFFFFF"
                android:gravity="center"
                android:orientation="vertical" >

                <Button
                    android:id="@+id/see_return_button2"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_margin="5dp"
                    android:background="#7CFC00"
                    android:text="See All"
                    android:textColor="#FFFFFF" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/layoutpostbutton1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >

        <Button
            android:id="@+id/restaurant_feedback_return_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginBottom="5dp"
            android:background="@drawable/round2"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="     Return     " />

    </RelativeLayout>

</RelativeLayout>